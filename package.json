{
  "name": "svarog-storyblok-integration",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "dev:debug": "webpack serve --mode development --open --env debug=true",
    "start": "node server/index.js",
    "start:server": "node server/index.js",
    "example": "npm run dev",
    "setup": "node scripts/setup-example.js",
    "storyblok:setup": "node scripts/storyblok-setup.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,html}\"",
    "validate": "npm run lint && npm run format:check && npm run test",
    "prepare": "npm run validate",
    "clean": "rm -rf dist/ coverage/",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "audit:fix": "npm audit fix",
    "audit:check": "npm audit --audit-level moderate",
    "build": "webpack --mode production",
    "build:analyze": "ANALYZE=true webpack --mode production",
    "build:report": "npm run build:analyze && open bundle-report.html",
    "lighthouse": "lighthouse http://localhost:3000 --view",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:serve": "npx http-server docs/generated -p 8080 -c-1",
    "docs:watch": "nodemon --watch src --ext js,md --exec npm run docs:generate",
    "docs:build": "npm run docs:generate && npm run docs:optimize",
    "docs:optimize": "node scripts/optimize-docs.js",
    "docs": "npm run docs:generate && npm run docs:serve",
    "cleanup": "node scripts/cleanup-template.js",
    "cleanup:dry-run": "DRY_RUN=true node scripts/cleanup-template.js",
    "demo:import": "node scripts/import-demo.js",
    "template:prepare": "npm run cleanup && npm run build",
    "template:verify": "node scripts/verify-template.js"
  },
  "size-limit": [
    {
      "path": "dist/js/*.js",
      "limit": "150 KB"
    },
    {
      "path": "dist/js/vendors*.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/js/svarog-ui*.js",
      "limit": "50 KB"
    }
  ],
  "dependencies": {
    "@svarog-ui/theme-default": "^1.2.2",
    "compression": "^1.8.0",
    "dompurify": "^3.2.6",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "morgan": "^1.10.0",
    "storyblok-js-client": "^6.7.0",
    "svarog-ui": "^4.1.8",
    "svarog-ui-core": "^1.2.4",
    "terser-webpack-plugin": "^5.3.10",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "compression-webpack-plugin": "^10.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.35.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/js": "^9.15.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.15.0",
    "globals": "^15.13.0",
    "happy-dom": "^17.4.7",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "style-loader": "^3.3.4",
    "vitest": "^3.1.4",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "keywords": [
    "svarog-ui",
    "storyblok",
    "cms",
    "javascript",
    "component-library",
    "integration",
    "example",
    "demo",
    "live-preview",
    "component-showcase"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/svarog-storyblok-integration.git"
  },
  "license": "MIT",
  "description": "Complete component showcase integration between Svarog-UI and Storyblok CMS",
  "author": "Your Name <your.email@example.com>",
  "homepage": "https://github.com/your-org/svarog-storyblok-integration#readme",
  "bugs": {
    "url": "https://github.com/your-org/svarog-storyblok-integration/issues"
  },
  "overrides": {
    "semver": "^7.6.0",
    "braces": "^3.0.3"
  }
}
