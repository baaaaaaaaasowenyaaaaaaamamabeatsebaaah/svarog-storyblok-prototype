    },
    {
      "path": "dist/js/vendors*.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/js/svarog-ui*.js",
      "limit": "50 KB"
    }
  ],
  "dependencies": {
    "@svarog-ui/theme-default": "^1.2.2",
    "compression": "^1.8.0",
    "dompurify": "^3.2.6",
