    "@vitest/ui": "^3.1.4",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.15.0",
    "globals": "^15.13.0",
    "happy-dom": "^17.4.7",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "style-loader": "^3.3.4",
    "vitest": "^3.1.4",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
