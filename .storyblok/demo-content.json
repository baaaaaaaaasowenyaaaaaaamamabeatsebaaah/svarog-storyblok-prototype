                "alignment": "center"
              },
              {
                "component": "grid",
                "_uid": "grid-themes",
                "columns": 12,
                "gap": "medium",
                "children": [
                  {
                    "component": "hero_section",
                    "_uid": "hero-theme-default",
                    "title": "Default Theme",
                    "subtitle": "Clean and professional",
                    "theme": "default"
                  },
                  {
                    "component": "hero_section",
                    "_uid": "hero-theme-cabalou",
                    "title": "Cabalou Theme",
                    "subtitle": "Modern and vibrant",
                    "theme": "cabalou"
                  },
                  {
                    "component": "hero_section",
                    "_uid": "hero-theme-muchandy",
